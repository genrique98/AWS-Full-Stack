{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "Storefront using Express and Postgres",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "start": "node ./index.js",
    "build-ts": "npx tsc",
    "watch": "tsc-watch --onSuccess 'node ./index.js'",
    "nodemon": "nodemon src/index.ts",
    "migrate": "db-migrate --env dev up",
    "test": "ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "deploy": "chmod +x ./bin/deploy.sh && ./bin/deploy.sh",
    "clean": "rm -rf dist/ || true",
    "build": "npm run clean && tsc && cp database.json dist/database.json && cp .npmrc dist/.npmrc && cp package.json dist/package.json && cd dist && zip -r Archive.zip . && cd .."
  },
  "author": "Gabriel Ramirez",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.981.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "supertest": "^6.1.6",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.11",
    "@types/express": "^4.17.12",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^15.12.4",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^2.0.11",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.11",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genrique98/SQL-Express-Storefront.git"
  },
  "bugs": {
    "url": "https://github.com/genrique98/SQL-Express-Storefront/issues"
  },
  "homepage": "https://github.com/genrique98/SQL-Express-Storefront#readme"
}
